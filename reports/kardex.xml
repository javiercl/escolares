<?xml version="1.0" encoding="utf-8"?>
<!--Custom report. -->
<odoo>
	<template id="kardex">
		<t t-call="web.html_container">
			<t t-foreach="docs" t-as="o">
				 	<div class="header"  style="font-size:10px">
						<strong>Header</strong>
				 	</div>

					<div class="page" style="font-size:10px">
						<table width="100%">
							<tr>
								<td width="10%">N.Control</td>
								<td width="40%">Nombre</td>
								<td width="10%">Sexo</td>
								<td width="40%">Carrera</td>
                            </tr>
							<tr>
								<td width="10%"><t t-esc="o.name"/></td>
								<td width="40%"><t t-esc="o.nombre"/></td>
								<td width="10%"><t t-esc="o.sexo"/></td>
								<td width="40%"><t t-esc="o.carrera_id.name"/></td>
                            </tr>
						</table>
					</div>
				 	<div class="footer"  style="font-size:9px">
						<strong>Footer</strong>
					</div>
			</t>
		</t>
		</template>
</odoo>